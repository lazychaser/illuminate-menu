{
    "name": "kalnoy/illuminate-menu",

    "description": "A menu builder for Laravel using Bootstrap's markup.",

    "keywords": [ "laravel", "menu", "bootstrap" ],

    "license": "MIT",

    "authors": [
        {
            "name": "Alexander Kalnoy",
            "email": "lazychaser@gmail.com"
        }
    ],

    "require": {
        "php": ">=5.4.0"
    },

    "require-dev": {
        "mockery/mockery": "~0.9",
        "illuminate/http": "4.2 - 5.5",
        "illuminate/support": "4.2 - 5.5"
    },

    "autoload": {
        "psr-4": {
            "Illuminate\\Html\\": "src/",
            "Illuminate\\Support\\Facades\\": "facade/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Illuminate\\Html\\MenuServiceProvider"
            ],

            "aliases": {
                "Menu": "Illuminate\\Support\\Facades\\Menu"
            }
        }
    },

    "minimum-stability": "stable"
}
